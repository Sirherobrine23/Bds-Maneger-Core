    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-prettify": "^0.1.1",
    "express-rate-limit": "^5.2.3",
    "googleapis": "^91.0.0",
    "inquirer": "^8.1.5",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.5",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.0.0",
    "oci-sdk": "^2.0.0",
    "ora": "^6.0.1",
    "properties-to-json": "^0.2.1",
    "request-ip": "^2.1.3",
