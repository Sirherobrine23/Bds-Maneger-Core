  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/The-Bds-Maneger/Bds-Maneger-Core/issues/new"
  },
  "homepage": "https://docs.bdsmaneger.com/Bds Maneger core",
  "engines": {
    "node": ">=15.6.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32",
    "android"
  ],
  "dependencies": {
    "@azure/storage-blob": "^12.6.0",
    "@iarna/toml": "^2.2.5",
    "adm-zip": "^0.5.1",
    "body-parser": "^1.19.0",
    "cli-color": "^2.0.0",
    "cors": "^2.8.5",
