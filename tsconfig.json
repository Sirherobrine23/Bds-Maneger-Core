{
  "include": [
    "src/",
    "src/download_server.ts"
  ],
  "exclude": ["node_modules/"],
  "compilerOptions": {
    "outDir": "./dist/cjs",
    "declaration": true,
    "strict": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "target": "ES6"
  }
}