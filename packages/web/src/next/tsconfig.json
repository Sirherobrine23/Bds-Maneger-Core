{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "ESNext",
    "lib": [
      "dom",
      "DOM.Iterable"
    ]
  },
  "exclude": [
    "next.config.cjs",
    "**/next.config.*js",
    "**/.next/"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
